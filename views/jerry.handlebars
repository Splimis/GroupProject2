<main>
<div class="col m4">
    <form action="/jerry" Method="GET">
      <ul>
        {{#each quote}}
          <li>
          <p>{{this.quotation}}</p>
          </li>
        {{/each}}
      </ul>
      <button type="submit" class="btn btn-primary newquote" name="" id="button">
      <p>Jerry Quote</p>
      </button>
    </form>
</div>
</main>