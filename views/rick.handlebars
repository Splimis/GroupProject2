<main>
<div class="col m4">
      <ul>
      <form action="/rick" Method="GET">
        {{#each quote}}
          <li>
          <p>{{this.quotation}}</p>
          </li>
        {{/each}}
      </ul> 
      <button type="submit" class="btn btn-primary newquote" name="" id="button">
      <p>Rick Quote</p>
      </button>
      </form>
    </div>
    <div class="player">
  
    {{#each quote}}
        <li>
        <p>{{{this.url}}}</p>
        </li>
    {{/each}}
    </div>
</main>

